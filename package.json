  "main": "n/a",
  "version": "2.1.1",
  "scripts": {
    "build": "npm run lint && npm run test && npm run gatsby:build && npm run storybook:build",
    "build:dangerous": "npm run gatsby:build && npm run storybook:build",
    "develop": "concurrently 'npm run gatsby' 'npm run storybook -- --quiet'",
    "develop:nossl": "concurrently 'npm run gatsby:nossl' 'npm run storybook:nossl -- --quiet'",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "gatsby": "gatsby develop --https --cert-file ./t485.org.pem --key-file ./t485.org-key.pem",
    "gatsby:build": "gatsby build",
    "gatsby:clean": "gatsby clean",
    "gatsby:nossl": "gatsby develop",
    "gatsby:serve": "gatsby serve",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'  --fix --cache && echo '\\033[1;32mLinting Passed' || echo '\\033[1;31mLinting Failed' && exit 0",
    "prettier": "prettier --write '**/*.{ts,tsx,js,jsx,json,md,scss,css}'",
    "start": "npm run gatsby",
    "start:nossl": "npm run develop:nossl",
    "storybook": "start-storybook -p 6006 --https --ssl-cert ./t485.org.pem  --ssl-key ./t485.org-key.pem -c ./storybook-config",
    "storybook:build": "build-storybook -o ./public/development/storybook/ -c ./storybook-config",
    "storybook:nossl": "start-storybook -p 6006 -c ./storybook-config",
    "test": "jest"
  },
